{
  "name": "establishment-service",
  "version": "1.0.0",
  "description": "Establishment service",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc --project ./ && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "clean": "rm -r dist",
    "start": "ts-node-dev --respawn --pretty --prefer-ts true -r tsconfig-paths/register --transpile-only src/app.ts",
    "docker:build": "docker build --no-cache -t establishments-service .",
    "docker:clean": "docker service rm establishments-service && docker rmi -f establishments-service",
    "deploy:local": "env ORIGIN=localhost docker stack deploy --compose-file=docker-compose.yml bukly",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:test": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix"
  },
  "author": "Giric Goyal",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@mysql/xdevapi": "^8.0.25",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.35",
    "@types/randomstring": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "crypto-random-string": "^4.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "moment": "^2.29.1",
    "prettier": "^2.3.2",
    "randomstring": "^1.2.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^3.0.1"
  }
}
